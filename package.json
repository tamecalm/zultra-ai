{
  "name": "@zultralabs/ai",
  "version": "1.3.0",
  "description": "Zultra is an intelligent ai-powered developer assistant for the terminal. it helps you generate, modify, and document code effortlessly, right from your command line.",
  "type": "module",
  "main": "./zultra.js",
  "bin": {
    "zultra": "./zultra.js"
  },
  "engines": {
    "node": ">18.0.0"
  },
  "scripts": {
    "dev": "node ./bin/zultra.ts",
    "build": "tsc",
    "build:binary": "node -e \"require('fs').mkdirSync('dist', {recursive:true})\" && nexe -i zultra.js -o dist/zultra -t windows-x64-12.16.2"
  },
  "keywords": [
    "cli",
    "ai",
    "automation",
    "developer-tools",
    "workflow",
    "zultra",
    "gemini",
    "developer-tool",
    "automation",
    "nodejs",
    "readme-generator",
    "code-assistant"
  ],
  "author": "John Ilesanmi",
  "license": "ISC",
  "files": [
    "zultra.js",
    "zultra/**/*",
    "README.md",
    "LICENSE",
    "SECURITY.md"
  ],
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "chalk": "^4.1.2",
    "commander": "^14.0.2",
    "diff": "^5.1.0",
    "fast-glob": "^3.3.2",
    "figlet": "^1.9.3",
    "highlight.js": "^11.9.0",
    "inquirer": "^8.2.7",
    "ora": "^5.4.1"
  },
  "devDependencies": {
    "@types/diff": "^5.0.9",
    "@types/figlet": "^1.5.8",
    "@types/inquirer": "^8.2.10",
    "@types/node": "^20.10.0",
    "nexe": "4.0.0-rc.7",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}